<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".MainActivity">

    <include
        android:id="@+id/toolbar"
        layout="@layout/layout_toobar" />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@+id/cheque_upload_btn"
        android:layout_below="@id/toolbar">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="5dp"
            android:orientation="vertical">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/open_sans_bold"
                android:padding="5dp"
                android:text="Cheque No"
                android:textColor="@color/black" />

            <EditText
                android:id="@+id/edTextChequeNo"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="5dp"
                android:background="@drawable/edit_text_register"
                android:fontFamily="@font/open_sans_bold"
                android:hint="Enter cheque No"
                android:inputType="number"
                android:maxLength="6"
                android:padding="10dp"
                android:textCursorDrawable="@drawable/color_cursor_black" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="5dp"
                android:textColor="@color/black"
                android:fontFamily="@font/open_sans_bold"
                android:text="Routing No"/>

            <EditText
                android:id="@+id/edTextRouting"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="5dp"
                android:background="@drawable/edit_text_register"
                android:fontFamily="More Fonts..."
                android:hint="Enter routing no"
                android:inputType="number"
                android:maxLength="9"
                android:padding="10dp"
                android:textCursorDrawable="@drawable/color_cursor_black" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="5dp"
                android:textColor="@color/black"
                android:fontFamily="@font/open_sans_bold"
                android:text="Short AC No"/>

            <EditText
                android:id="@+id/edTextShortAcNo"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="5dp"
                android:background="@drawable/edit_text_register"
                android:hint="Enter short ac no"
                android:maxLength="6"
                android:inputType="number"
                android:padding="10dp"
                android:fontFamily="@font/open_sans"
                android:textCursorDrawable="@drawable/color_cursor_black" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="5dp"
                android:fontFamily="@font/open_sans_bold"
                android:textColor="@color/black"
                android:text="TC No"/>

            <EditText
                android:id="@+id/edTexttC"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="5dp"
                android:background="@drawable/edit_text_register"
                android:hint="Enter TC no"
                android:fontFamily="@font/open_sans"
                android:maxLength="2"
                android:inputType="number"
                android:padding="10dp"
                android:textCursorDrawable="@drawable/color_cursor_black" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="5dp"
                android:textColor="@color/black"
                android:fontFamily="@font/open_sans_bold"
                android:text="IFSC Code"/>

            <EditText
                android:id="@+id/edTextIfscCode"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="5dp"
                android:background="@drawable/edit_text_register"
                android:hint="Enter IFSC no"
                android:maxLength="11"
                android:inputType="textPhonetic"
                android:fontFamily="@font/open_sans"
                android:padding="10dp"
                android:textCursorDrawable="@drawable/color_cursor_black" />


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="5dp"
                android:textColor="@color/black"
                android:fontFamily="@font/open_sans_bold"
                android:text="BOFD No"/>

            <EditText
                android:id="@+id/edTextBofd"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="5dp"
                android:background="@drawable/edit_text_register"
                android:hint="Enter BOFD no"
                android:fontFamily="@font/open_sans"
                android:maxLength="9"
                android:inputType="number"
                android:padding="10dp"
                android:textCursorDrawable="@drawable/color_cursor_black" />


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="5dp"
                android:fontFamily="@font/open_sans_bold"
                android:textColor="@color/black"
                android:text="Account No"/>

            <EditText
                android:id="@+id/edTexttAccountNo"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="5dp"
                android:background="@drawable/edit_text_register"
                android:hint="Enter account no"
                android:fontFamily="@font/open_sans"
                android:maxLength="20"
                android:inputType="number"
                android:padding="10dp"
                android:textCursorDrawable="@drawable/color_cursor_black" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="5dp"
                android:textColor="@color/black"
                android:fontFamily="@font/open_sans_bold"
                android:text="Mobile No"/>

            <EditText
                android:id="@+id/edTexttMobileNo"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="5dp"
                android:background="@drawable/edit_text_register"
                android:hint="Enter mobile no"
                android:fontFamily="@font/open_sans"
                android:maxLength="10"
                android:inputType="phone"
                android:padding="10dp"
                android:textCursorDrawable="@drawable/color_cursor_black" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="5dp"
                android:textColor="@color/black"
                android:fontFamily="@font/open_sans_bold"
                android:text="Cheque amount"/>

            <EditText
                android:id="@+id/edTexttChequeAmt"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="5dp"
                android:background="@drawable/edit_text_register"
                android:hint="Enter cheque amount"
                android:fontFamily="@font/open_sans"
                android:inputType="number"
                android:maxLength="15"
                android:padding="10dp"
                android:textCursorDrawable="@drawable/color_cursor_black" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="5dp"
                android:textColor="@color/black"
                android:fontFamily="@font/open_sans_bold"
                android:text="Account name"/>

            <EditText
                android:id="@+id/edTexttAccountName"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="5dp"
                android:fontFamily="@font/open_sans"
                android:background="@drawable/edit_text_register"
                android:hint="Enter account name"
                android:inputType="textPersonName"
                android:padding="10dp"
                android:textCursorDrawable="@drawable/color_cursor_black" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="5dp"
                android:textColor="@color/black"
                android:fontFamily="@font/open_sans_bold"
                android:text="Cheque date"/>

            <TextView
                android:id="@+id/edTexttChequeDate"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="5dp"
                android:background="@drawable/edit_text_register"
                android:gravity="center_vertical"
                android:fontFamily="@font/open_sans"
                android:hint="DD/MM/YYYY"
                android:inputType="date"
                android:padding="10dp"
                android:text="01/01/2023"
                android:textCursorDrawable="@drawable/color_cursor_black" />
        </LinearLayout>
    </ScrollView>


    <Button
        android:id="@+id/cheque_upload_btn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Upload Cheque"
        android:fontFamily="@font/open_sans_bold"
        android:layout_alignParentBottom="true"
        android:layout_margin="10dp"
        android:background="@drawable/home_btn"
        android:textColor="@color/white" />

</RelativeLayout>

